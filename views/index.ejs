              <!-- Latest Start -->

              <section class="pb-4 pt-4 ">
                <div class="d-flex mb-2 align-items-center">
                  <h2> احدث الاضافات </h2>
                  <a href="/latest" class="ms-auto">عرض المزيد</a>
                </div>

                <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
                  <% if(pros !='' ) { %>
                    <% pros.latest.forEach(function(prodect, index){ %>
                      <a href="/prodects/<%= prodect.id %>" class="col text-center category__link">
                        <div class="category__img category__img--large shadow">
                          <img src="<%= prodect.image[0] %>" alt="<%= prodect.name %>" loading="lazy">
                        </div>
                        <div class="pt-1">
                          <%= prodect.name %>
                        </div>
                        <div class="pt-1 text-danger">
                          <%= prodect.prise + " ج س" %>
                         </div>
                      </a>
                      <% }) %>
                        <% } else { %>
                          <p>لا يوجد سلع حاليا</p>
                          <% } %>
                </div>

