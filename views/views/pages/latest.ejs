<%- include ('../partials/_dashboardHeader') %>

  <div class="container-fluid">
    <div class="row">
      <%- include ('../partials/_sidebarmenu') %>
        <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">



          <%- include ('../partials/_ads') %>


            <h1 class="pb-4">احدث الاضافات</h1>

            <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
              <% if(prodect !='' ) { %>
                <% prodect.forEach(function(pro, index){ %>
                  <a href="/prodects/<%= pro._id %>" class="col text-center category__link">
                    <div class="category__img category__img--large shadow">
                      <img src="<%= pro.image %>" alt="<%= pro.name %>" loading="lazy">
                    </div>
                    <div class="pt-1">
                      <%= pro.name %>
                    </div>
                  </a>
                  <% }) %>
                    <% } else { %>
                      <p>No items found.</p>
                      <% } %>
            </div>


        </main>
    </div>
  </div>