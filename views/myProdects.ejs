<%- include ('./partials/_dashboardHeader') %>

    <div class="container-fluid">
        <div class="row">

            <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
                <%- include ('./partials/_sidebarmenu') %>



                    <!--  Start -->
                    <section class="pb-4 pt-4">
                        <div class="d-flex mb-2 align-items-center">
                            <h2>سلع <%= user.name %>
                            </h2>

                        </div>

                        <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
                            <% march.forEach(e=> { %>

                                <a href="/prodects/<%= e._id %>" class="col text-center category__link1">
                                    <div class="category__img1 category__img--large">
                                        <img src="<%= e.image %>" width="100%" alt="<%= e.name %> "
                                            loading="lazy">

                                    </div>
                                    <div class="pt-1">
                                        <%= e.name %>
                                    </div>
                                </a>
                                <% }) %>

                        </div>
                    </section>





        </div>
    </div>
    </div>

